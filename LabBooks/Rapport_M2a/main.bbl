  \verb 10.1117/12.238732
  \endverb
  \field{isbn}{081942031X}
  \field{issn}{0277786X}
  \field{pages}{350\bibrangedash 360}
  \field{title}{{Implementation of a foveated image coding system for image
  bandwidth reduction}}
  \field{volume}{2657}
