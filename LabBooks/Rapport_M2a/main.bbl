  \field{issn}{10976256}
  \field{number}{9}
  \field{pages}{1195\bibrangedash 1204}
  \field{title}{{Metamers of the ventral stream}}
  \field{volume}{14}
  \verb{file}
  \verb :home/pimt/Documents/Articles/Freeman2011.pdf:pdf
  \endverb
  \field{journaltitle}{Nature Neuroscience}
  \field{year}{2011}
\endentry

